{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"

        },
        {
           "type": "package",
           "package": {
           "name": "d3/d3",
           "version": "v3.5.17",
           "type": "drupal-library",
           "dist": {
            "url": "https://github.com/d3/d3/archive/v3.5.17.zip",
            "type": "zip"
                }
            }
         },
         {
          "type": "package",
          "package": {
          "name": "components/highlightjs",
          "version": "9.7.0",
          "type": "drupal-library",
          "dist": {
            "url": "https://github.com/components/highlightjs/archive/9.7.0.zip",
            "type": "zip"
                   }
                   }
          }

    ],
    "require": {
        "php": ">=5.6",
        "components/highlightjs": "9.7.*",
        "d3/d3": "3.5.*",
        "drupal/address": "^1.4",
        "drupal/admin_toolbar": "3.x-dev@dev",
        "drupal/adminimal_theme": "^1.4",
        "drupal/auto_entitylabel": "^3.0",
        "drupal/bootstrap_barrio": "^5.5",
        "drupal/bootstrap_library": "^2.0",
        "drupal/captcha": "^1.0@beta",
        "drupal/computed_field": "^3.0@alpha",
        "drupal/conditional_fields": "^4.0@alpha",
        "drupal/console": "~1.0",
        "drupal/core": "^9.5.11",
        "drupal/core-composer-scaffold": "^9.2",
        "drupal/core-dev": "^9.2",
        "drupal/csv_importer": "1.x-dev",
        "drupal/ctools": "^3.7",
        "drupal/ds": "^3.2",
        "drupal/email_registration": "1.x-dev",
        "drupal/entity_browser": "^2.1",
        "drupal/entity_usage": "^2.0@alpha",
        "drupal/facets": "^2.0",
        "drupal/field_group": "^3.0",
        "drupal/geofield": "^1.1",
        "drupal/google_analytics": "^4.0",
        "drupal/google_tag": "^1.4",
        "drupal/imce": "^3.0",
        "drupal/jquery_ui_slider": "^2.0",
        "drupal/jquery_ui_touch_punch": "^1.0",
        "drupal/mailgun": "^2.0",
        "drupal/memcache": "^2.0",
        "drupal/menu_block": "^1.5",
        "drupal/menu_breadcrumb": "^2.0@alpha",
        "drupal/migrate_source_csv": "^3.4",
        "drupal/nodeaccess": "^2.0@alpha",
        "drupal/page_manager": "^4.0@beta",
        "drupal/panels": "^4.3",
        "drupal/paragraphs": "^1.8",
        "drupal/pathauto": "^1.6",
        "drupal/profile": "^1.1",
        "drupal/recaptcha": "^3.0",
        "drupal/scheduler": "^1.0",
        "drupal/search_api": "^1.28",
        "drupal/search_api_db": "^1.12",
        "drupal/search_api_page": "^1.0@alpha",
        "drupal/simple_mail": "^2.0",
        "drupal/smtp": "^1.0@beta",
        "drupal/subpathauto": "^1.0@RC",
        "drupal/superfish": "^1.3",
        "drupal/token": "^1.5",
        "drupal/token_custom": "^1.0@alpha",
        "drupal/token_filter": "^2.1",
        "drupal/upgrade_status": "^4.0",
        "drupal/viewfield": "^3.0@alpha",
        "drupal/views_bulk_operations": "^4.0@RC",
        "drupal/views_data_export": "^1.0@beta",
        "drupal/workflow": "^1.1",
        "drush/drush": "^10.3",
        "egulias/email-validator": "^2.1.22",
        "pear/archive_tar": "^1.4.10",
        "php-http/guzzle6-adapter": "^2.0",
        "symfony/debug": "^4.4.19",
        "symfony/dependency-injection": "^4.4",
        "symfony/event-dispatcher": "^4.4",
        "symfony/filesystem": "^4.4",
        "symfony/finder": "^4.4",
        "symfony/http-foundation": "^4.4.7",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "php-http/discovery": true,
            "drupal/console-extend-plugin": true,
            "drupal/core-composer-scaffold": true
        }
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/{$name}": ["type:drupal-drush"]
        },
        "drupal-scaffold": {
          "locations": {
              "web-root": "web/"
          },
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            }
        }
    }
}
